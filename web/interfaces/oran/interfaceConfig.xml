<interfaceConfig>
  <actionList>
    <action name='p' class='PageAction'>
      <subaction name='home' xslt='pages/home.xsl'/>
      <subaction name='about' xslt='pages/about.xsl'/>
      <subaction name='pref' xslt='pages/pref.xsl'/>
      <subaction name='help' xslt='pages/help.xsl'/>
      <subaction name='login' xslt='pages/login.xsl'/>
      <subaction name='gli4gs3' xslt='gli4gs3.xsl'/>
      <subaction name='nav' xslt='nav.xsl'/><!-- used for the collection header frame -->
      <subaction name="html" xslt="html.xsl"/> <!-- use to put an external page into a frame with a collection header-->
    </action>
    <action name='q' class='QueryAction' xslt='pages/query.xsl'/>
