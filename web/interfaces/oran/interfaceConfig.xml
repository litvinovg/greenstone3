    <action name='a' class='AppletAction' xslt='applet.xsl'/>
    <action name='xd' class='XMLDocumentAction'>
      <subaction name='toc' xslt='gberg/document-toc.xsl'/>
      <subaction name='text' xslt='gberg/document-content.xsl'/>
    </action>
    <action name='pr' class='ProcessAction' xslt='process.xsl'/>
    <action name='s' class='SystemAction' xslt='system.xsl'/>

    <action name='f' class='FormatAction'/> <!-- xslt='formatString.xsl'/> -->

    <action name='g' class='GeneralAction'>
      <subaction name="authen" xslt='pages/authen.xsl'/>
      <subaction name="berry" xslt='pages/berry.xsl'/>
	  <subaction name="documentbasket" xslt='pages/documentbasket.xsl'/>
    </action>
  </actionList>
